class Solution:
    def subsets(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        result = []
        
        def backtrack(start, path):
            # Add the current subset
            result.append(path[:])
            
            for i in range(start, len(nums)):
                # Include nums[i]
                path.append(nums[i])
                backtrack(i + 1, path)
                # Backtrack
                path.pop()
        
        backtrack(0, [])
        return result
